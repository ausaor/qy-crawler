          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697083259071.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "技能语音1-3"
        },
        {
          "yywbzt_1517": "折转",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697083476895.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "技能语音1-4"
        },
        {
          "yywbzt_1517": "观心",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697083686414.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "技能语音2-1"
        },
        {
          "yywbzt_1517": "以静制动",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697084279404.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "技能语音2-2"
        },
        {
          "yywbzt_1517": "心如止水",

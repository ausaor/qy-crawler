{
  "yxmczt_3242": "赵怀真",
  "yxidzt_6368": "544",
  "dqpfyy_5403": [
    {
      "pffmtm_4757": "//game.gtimg.cn/images/yxzj/zlkdatasys/images/image/20221130/16697995453542.jpg",
      "pffmtm_8153": "//game.gtimg.cn/images/yxzj/zlkdatasys/images/image/20221130/16697995529345.jpg",
      "yylbzt_9132": [
        {
          "yywbzt_1517": "心在寰宇，自得自在。",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697121099535.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "大厅语音1"
        },
        {
          "yywbzt_1517": "红尘万象，诸多烦恼，既是修行，也是乐事。",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697121264466.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "大厅语音2"
        },
        {
          "yywbzt_1517": "生有时，死有命，顺本心，当无憾。",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697121476593.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "大厅语音3"
        },
        {
          "yywbzt_1517": "人非草木，孰能无情。",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697071223438.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "移动语音1"
        },
        {
          "yywbzt_1517": "生和死，我早已徘徊过无数次。",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697071754037.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "移动语音2"
        },
        {
          "yywbzt_1517": "人生一世，总有许多不能十全十美之事。",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697072081945.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "移动语音3"
        },
        {
          "yywbzt_1517": "不为无益之事，何以悦有涯之生。",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697072406688.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "移动语音4"
        },
        {
          "yywbzt_1517": "阴阳变化万千，大道亘古恒常。",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697072651339.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "移动语音5"
        },
        {
          "yywbzt_1517": "就算欲望再多，也不要迷失本心。",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697072966986.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "移动语音6"
        },
        {
          "yywbzt_1517": "人生天地，若白驹过隙，忽然而已（选自庄周《庄子·外篇·知北游》）",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697074215973.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "移动语音7"
        },
        {
          "yywbzt_1517": "生死悠悠尔，一气聚散之。（选自柳宗元《掩役夫张进骸》）",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697074459970.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "移动语音8"
        },
        {
          "yywbzt_1517": "人生如寄，皆是归人。",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697074973969.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "移动语音9"
        },
        {
          "yywbzt_1517": "修行，亦是修心。",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697075379342.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "移动语音10"
        },
        {
          "yywbzt_1517": "能见广阔天地之眼，必是低首俯察之目。",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697075599504.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "移动语音11"
        },
        {
          "yywbzt_1517": "尊驾来如风雨，为何去似微尘。",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697076075626.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "挑衅"
        },
        {
          "yywbzt_1517": "一气融结，万象胚浑。",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697076641563.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "击杀语音1"
        },
        {
          "yywbzt_1517": "大象无形，道隐无名。（选自《道德经》第四十一章）",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697077344342.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "击杀语音2"
        },
        {
          "yywbzt_1517": "和其光，同其尘。",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697077541296.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "击杀语音3"
        },
        {
          "yywbzt_1517": "芸芸众生，各复其归根。",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697077767628.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "大招击杀（高光）"
        },
        {
          "yywbzt_1517": "对不起，我食言了……",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697078049734.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "死亡语音1"
        },
        {
          "yywbzt_1517": "气散风止……",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697078254954.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "死亡语音2"
        },
        {
          "yywbzt_1517": "返璞归真……",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697080655387.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "死亡语音3"
        },
        {
          "yywbzt_1517": "云气来",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697082837604.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "技能语音1-1"
        },
        {
          "yywbzt_1517": "秋风扫落叶",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20221129/16697083017973.mp3",
          "ssjqmc_1356": "",
          "yygn_8632": "技能语音1-2"
        },
        {

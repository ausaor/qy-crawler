          "ssjqmc_1356": "",
          "yygn_8632": "技能语音1"
        },
        {
          "yywbzt_1517": "有些伤害比我能看见的远",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20220407/16493196553983.wav",
          "ssjqmc_1356": "",
          "yygn_8632": "技能语音1_1"
        },
        {
          "yywbzt_1517": "别堵在那，请让下路",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20220407/16493197795100.wav",
          "ssjqmc_1356": "",
          "yygn_8632": "技能语音2"
        },
        {
          "yywbzt_1517": "学会沉默吧",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20220407/16493199493565.wav",
          "ssjqmc_1356": "",
          "yygn_8632": "技能语音2_1"
        },
        {
          "yywbzt_1517": "骄傲值得分享",
          "yywjzt_5304": "//game.gtimg.cn/images/yxzj/zlkdatasys/audios/audio/20220407/16493200398565.wav",
          "ssjqmc_1356": "",
          "yygn_8632": "技能语音3"
        },
        {
          "yywbzt_1517": "理解胜于一切！",
